public class App {
    public static void main(String[] args) throws Exception {

        for (int i = 5; i != 0; i--) {
            System.out.println("joshua");
        }
    
    
               

        int nickNameCount = 5;

        while (nickNameCount != 0) {
        System.out.println("pogi");
        nickNameCount--;
        }
                
    
        int lastNameCount = 5;

        do {
            System.out.println("Labayne");
            lastNameCount--;
        } while (lastNameCount != 0);
    }
}
